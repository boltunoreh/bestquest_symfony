{% extends 'base.html.twig' %}

{% block header %}
    <header class='l-header'>
        <div class='l-header__inner'>
            <a class='logo-wrapper unvisible' href='/' id='logotype'>
                <h1 class='logotype'>
                    <span class='active'>Best Quest</span>
                    <span>Настоящие Квесты</span>
                </h1>
            </a>
            <nav class='l-header__nav'>
                <ul class='l-header__menu'>
                    <li class='item'>
                        <a href="/">Проекты</a>
                        <ul class='l-header__submenu js-slider-control'>
                            {% for category in categories %}
                                <li class='item' data-slider="{{ category.title }}">
                                    <i>{{ category.title }}</i></li>
                            {% endfor %}
                            <li class='item' data-slider="excl"><i>Эксклюзивный проект</i></li>
                        </ul>
                    </li>
                    <li class='item'>
                        <a href="{{ path('app__about') }}">О компании</a>
